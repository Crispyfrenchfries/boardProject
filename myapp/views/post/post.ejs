<% include ../top.html%>
    <% for(var i=0; i<post.length; i++) {%>
    <div class="card">
      <div class="card-body"><h2><%= post[i].title %></h2></div>
    </div>
    <div class="card">
        <div class="card-body">시간:<%= post[i].datetime %></div>
    <div class="card">
      <div class="card-body"><%= post[i].description %></div>
    <div class="btn-group">
      <form action="/post/delete_process" method="post">
        <button type="button" onclick="location.href='/post/create' " class="btn btn-primary">글쓰기</button>
        <button type="button" onclick="location.href='/post/update?id=<%=post[i].index_post%> ' " class="btn btn-primary">수정</button>
        <input type="hidden" name="id" value="<%= post[i].index_post%>">
        <button type="submit" class="btn btn-primary">삭제</button>
      </form>
    </div>  
  <% } %>
<%include ../footer.html%>




